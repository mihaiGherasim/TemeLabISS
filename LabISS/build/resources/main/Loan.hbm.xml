<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name = "model.Loan" table="imprumut">
        <id name="id" column="id">
            <generator class="increment"></generator>
        </id>
        <property name="isbn" type="java.lang.Integer" column="isbn"/>
        <property name="noBooks" type="java.lang.Integer" column="nr_carti"/>
        <property name="name" type="java.lang.String" column="nume"/>
        <property name="email" type="java.lang.String" column="email"/>
        <property name="dayLoan" type="java.lang.Integer" column="zi_imprumut"/>
        <property name="monthLoan" type="java.lang.Integer" column="luna_imprumut"/>
        <property name="yearLoan" type="java.lang.Integer" column="an_imprumut"/>
    </class>
</hibernate-mapping>
