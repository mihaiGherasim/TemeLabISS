<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name = "model.User" table="abonat">
        <id name="id" column="id">
            <generator class="increment"></generator>
        </id>
        <property name="cnp" column="cnp"/>
        <property name="nume" type="java.lang.String" column="nume"/>
        <property name="adresa" type="java.lang.String" column="adresa"/>
        <property name="telefon" type="java.lang.String" column="telefon"/>
        <property name="parola" type="java.lang.String" column="parola"/>
    </class>
</hibernate-mapping>
